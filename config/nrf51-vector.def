IRQN(9, TIMER1, timer1)
IRQN(10, TIMER2, timer2)
IRQN(11, RTC0, rtc0)
IRQN(12, TEMP, temp)
IRQN(13, RNG, rng)
IRQN(14, ECB, ecb)
IRQN(15, ARR_CCM, aar_ccm)
IRQN(16, WDT, wdt)
